{
  "name": "tourze/quic-workerman",
  "description": "QUIC协议Workerman集成包",
  "type": "library",
  "license": "MIT",
  "require": {
    "ext-hash": "*",
    "symfony/dependency-injection": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/phpunit-base": "1.*",
    "tourze/quic-transport": "0.0.*",
    "workerman/workerman": "^5.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\QUIC\\Workerman\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\QUIC\\Workerman\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}